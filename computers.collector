<?php

if (isset($data) && isset($type)) {
    if (isset($_['time'])) {
        $time = $_['time'];
    } else {
        $time = time();
    }

    $state = $_['state'];
    $data = explode("/", $data);
    $data = $data[2];


    Data::saveData('', $data, $type, $time, $state, MAXDATA);
    Data::checkTriggers($data, $type, $time, $state);
}
