{
   "off" : {
      "command" : "sudo /opt/piget/computers/computersOff",
      "usage" : "computers ipaddress password type",
      "args" : {
         "type" : {
            "type" : "Config"
         },
         "ipaddress" : {
            "type" : "Config"
         },
         "password" : {
            "type" : "Config"
         }
      }
   },
   "on" : {
      "command" : "sudo /opt/piget/computers/computersOn",
      "usage" : "computers macaddress",
      "args" : {
         "macaddress" : {
            "type" : "Config"
         }
     }
   }
}
